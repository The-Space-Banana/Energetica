{
    "files.associations": {
        "*.jinja": "jinja-html",
        "*.js": "javascript"
    },
    "[jinja-html]": {
        "editor.wordWrapColumn": 80,
        "editor.defaultFormatter": "monosans.djlint"
    },
    "[javascript]": {
        "editor.wordWrapColumn": 120,
        "editor.rulers": [120]
    },
    "files.exclude": {
        "**/__pycache__": true
    }
}
